{
  "schemaVersion": "2.2",
  "description": "Apply past monthly security updates",
  "mainSteps": [
    {
      "action": "aws:runShellScript",
      "name": "runShellScript",
      "inputs": {
        "runCommand": [
          "aws",
          "ssm",
          "create-patch-baseline",
          "--operation",
          "Install",
          "--name",
          "YourPatchBaselineName"  // Replace with your patch baseline name
        ]
      }
    }
  ]
}
