image: 

stage:
  - validate
  - deploy

##### Validate #####

validate:
    stage: validate
    script:
          - 

##### Deploy #####

deploy:
  stage: deplot
  variables:
    stackname: strapi-cf.yml
script:
    - aws configure set aws_access_key_id $AWS_ACCESS_KEY_ID
    - aws configure set aws_secret_access_key $AWS_SECRET_ACCESS_KEY
    - aws configure set region us-gov-west-1  # Set your desired AWS region
    - aws cloudformation deploy \
        --stack-name strapi-cf.yml \
        --template-file path/to/your/template.yml \
        --capabilities CAPABILITY_IAM



